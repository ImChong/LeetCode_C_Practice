    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "#000000",
        "background": "#ffffff",
        "opacity": 50,
        "iconColour": "#6c6c6c",
        "gutterIcon": true
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "foreground": "#000000",
            "background": "#fff719",
            "iconColour": "#fff719"
        },
        "NOTE": {
            "icon": "note",
            "foreground": "#F0EDE1",
            "background": "#6c6c6c",
            "iconColour": "#6c6c6c"
        },
        "COMMENT": {
            "icon": "note",
            "foreground": "#F0EDE1",
            "background": "#6c6c6c",
            "iconColour": "#6c6c6c"
        },
        "FIXME": {
            "foreground": "#000000",
            "background": "#FF7F50",
            "iconColour": "#FF7F50"
        },
        "BUG": {
            "icon": "bug",
            "foreground": "#F0EDE1",
            "background": "#ff4747",
            "iconColour": "#ff4747"
        },
        "[ ]": {
            "icon": "check",
            "foreground": "#000000",
            "background": "#F0EDE1",
            "iconColour": "#F0EDE1"
        },
        "[x]": {
            "icon": "check",
            "foreground": "#F0EDE1",
            "background": "#32a852",
            "iconColour": "#32a852"
        },
        "DONE": {
            "icon": "check",
            "foreground": "#F0EDE1",
            "background": "#32a852",
            "iconColour": "#32a852"
        },
        "#if": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "#F0EDE1",
            "background": "#0061e8",
            "iconColour": "#0061e8"
            // "gutterIcon": false,
        },
        "#endif": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "#F0EDE1",
            "background": "#0061e8",
            "iconColour": "#0061e8"
            // "gutterIcon": false,
        },
        "#elif": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "#F0EDE1",
            "background": "#1085c9",
            "iconColour": "#1085c9"
            // "gutterIcon": false,
        },
        "#else": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "#F0EDE1",
            "background": "#1085c9",
            "iconColour": "#1085c9"
            // "gutterIcon": false,
        },
        "Functionality": {
            "icon": "note",
            "type": "line",
            "foreground": "#F0EDE1",
            "background": "#32a852",
            "iconColour": "#32a852"
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "NOTE",
        "COMMENT",
        "[ ]",
        "[x]",
        "DONE",
        "#if",
        "#endif",
        "#elif",
        "#else",
        "Functionality"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "cmake.configureOnOpen": false,
